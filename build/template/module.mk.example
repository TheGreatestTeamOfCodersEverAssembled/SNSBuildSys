#_modulename_DHEXTRA :=

# _modulename_CF -- "CINT flags"
# 
# Extra flags to pass to rootcint. Mainly used to add "-p".
#_modulename_CF := -p

# _modulename_LIBEXTRA -- "Extra libraries"
# 
# List of extra libraries to be used when linking the shared library
# for this module.
#_modulename_LIBEXTRA := 

